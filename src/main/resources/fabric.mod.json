{
  "schemaVersion": 1,
  "id": "etherology",
  "version": "${modVersion}",
  "name": "Etherology",
  "description": "Science is indistinguishable from magic!",
  "authors": [
    "Syorito Hatsuki",
    "Faradey",
    "Feytox"
  ],
  "contact": {
    "issues": "https://github.com/syorito-hatsuki/etherology/issues",
    "sources": "https://github.com/syorito-hatsuki/etherology"
  },
  "license": "GNU Lesser General Public License v2.1\n",
  "icon": "assets/etherology/icon.png",
  "environment": "*",
  "accessWidener": "etherology.accesswidener",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "ru.feytox.etherology.Etherology"
      }
    ],
    "client": [
      {
        "value": "ru.feytox.etherology.EtherologyClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "ru.feytox.etherology.datagen.EtherologyDataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "custom": {
    "modmenu": {
      "badges": [],
      "update_checker": true
    }
  },
  "mixins": [
    "etherology.mixins.json",
    {
      "config": "etherology.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "*",
    "java": "*"
  }
}